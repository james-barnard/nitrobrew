export RUBY_VERSION=ruby-2.4.1
export PATH=/usr/local/rvm/gems/ruby-2.4.1/bin:/usr/local/rvm/gems/ruby-2.4.1@global/bin:/usr/local/rvm/rubies/ruby-2.4.1/bin:/usr/local/rvm/bin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin

cd /home/niki/nitrobrew
/usr/local/rvm/gems/ruby-2.4.1/bin/bundle exec bin/nitro.rb
